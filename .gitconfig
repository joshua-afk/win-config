[user]
	email = jr.velasquez@jeonsoft.com
	name = Joshua Ryan V. Velasquez
[github]
    user = joshua-afk
[core]
	editor = nvim
	autocrlf = true
	excludesfile = C:\\Users\\jr.velasquez\\Documents\\gitignore_global.txt
[alias]
    fuckit = reset --hard
    dontfuck = reset --soft
    ci = commit
    p = push
    st = status
    cm = commit -m
    co = checkout
    lol = log --graph --decorate --pretty=oneline --abbrev-commit
    lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
    plog = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	cfc = show --pretty --name-only
[color]
    ui = true
    branch = auto
    diff = auto
    interactive = auto
    status = auto
[credential]
	helper = store
	helper = cache --timeout=3600
[credential "http://main:1880"]
	provider = generic
[difftool "sourcetree"]
	cmd = '' \"$LOCAL\" \"$REMOTE\"
[mergetool "sourcetree"]
	cmd = "'' "
	trustExitCode = true
